{
  "dashboard": {
    "title": "Feature Store Monitoring (Feast)",
    "tags": ["feast", "feature-store", "mlops"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Feature Retrieval Latency (p50, p95, p99)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(feature_retrieval_latency_seconds_bucket[5m]))",
            "legendFormat": "p50 - {{feature_type}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(feature_retrieval_latency_seconds_bucket[5m]))",
            "legendFormat": "p95 - {{feature_type}}",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(feature_retrieval_latency_seconds_bucket[5m]))",
            "legendFormat": "p99 - {{feature_type}}",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Latency"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "title": "Feature Coverage (Success Rate)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "feature_coverage_ratio",
            "legendFormat": "{{feature_type}} coverage",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percentunit", "label": "Coverage", "min": 0, "max": 1},
          {"format": "short"}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [0.95], "type": "lt"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "name": "Low Feature Coverage",
          "message": "Feature coverage dropped below 95%"
        }
      },
      {
        "id": 3,
        "title": "Feature Retrieval Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "rate(feature_retrieval_total{status=\"success\"}[5m])",
            "legendFormat": "{{feature_type}} - success",
            "refId": "A"
          },
          {
            "expr": "rate(feature_retrieval_total{status=\"not_found\"}[5m])",
            "legendFormat": "{{feature_type}} - not found",
            "refId": "B"
          },
          {
            "expr": "rate(feature_retrieval_total{status=\"error\"}[5m])",
            "legendFormat": "{{feature_type}} - error",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "Feature Retrieval Status Breakdown",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "sum by (status) (rate(feature_retrieval_total[5m]))",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 5,
        "title": "Feature Store SLI: Latency < 10ms",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "sum(rate(feature_retrieval_latency_seconds_bucket{le=\"0.01\"}[5m])) / sum(rate(feature_retrieval_latency_seconds_count[5m]))",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {"values": false, "calcs": ["lastNotNull"]},
          "text": {"valueSize": 48},
          "graphMode": "area",
          "colorMode": "value",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "red"},
              {"value": 0.95, "color": "yellow"},
              {"value": 0.99, "color": "green"}
            ]
          }
        },
        "fieldConfig": {
          "defaults": {"unit": "percentunit", "min": 0, "max": 1}
        }
      },
      {
        "id": 6,
        "title": "Total Feature Retrievals",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 16},
        "targets": [
          {
            "expr": "sum(feature_retrieval_total)",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {"values": false, "calcs": ["lastNotNull"]},
          "text": {"valueSize": 48},
          "graphMode": "area",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {"unit": "short"}
        }
      },
      {
        "id": 7,
        "title": "Feature Not Found Rate",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "sum(rate(feature_retrieval_total{status=\"not_found\"}[5m])) / sum(rate(feature_retrieval_total[5m]))",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {"values": false, "calcs": ["lastNotNull"]},
          "text": {"valueSize": 48},
          "graphMode": "area",
          "colorMode": "value",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 0.05, "color": "yellow"},
              {"value": 0.1, "color": "red"}
            ]
          }
        },
        "fieldConfig": {
          "defaults": {"unit": "percentunit", "min": 0, "max": 1}
        }
      },
      {
        "id": 8,
        "title": "Avg Feature Retrieval Latency",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 16},
        "targets": [
          {
            "expr": "rate(feature_retrieval_latency_seconds_sum[5m]) / rate(feature_retrieval_latency_seconds_count[5m])",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {"values": false, "calcs": ["lastNotNull"]},
          "text": {"valueSize": 48},
          "graphMode": "area",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {"unit": "s", "decimals": 4}
        }
      }
    ],
    "refresh": "10s",
    "time": {"from": "now-1h", "to": "now"},
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"]
    }
  }
}
